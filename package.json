{
    "name": "@lemgenda/image-lemgendizer",
    "version": "2.2.1",
    "description": "Client-side batch image processing library",
    "type": "module",
    "main": "./dist/index.cjs.js",
    "module": "./dist/index.es.js",
    "exports": {
        ".": {
            "import": "./dist/index.es.js",
            "require": "./dist/index.cjs.js"
        },
        "./utils": {
            "import": "./dist/utils/index.js",
            "require": "./dist/utils/index.cjs.js"
        },
        "./processors": {
            "import": "./dist/processors/index.js",
            "require": "./dist/processors/index.cjs.js"
        },
        "./templates": {
            "import": "./dist/templates/index.js",
            "require": "./dist/templates/index.cjs.js"
        }
    },
    "files": [
        "dist",
        "README.md"
    ],
    "scripts": {
        "dev": "vite",
        "build": "vite build",
        "preview": "vite preview"
    },
    "keywords": [
        "image",
        "processing",
        "browser",
        "client-side"
    ],
    "author": "LemGenda",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/lemgenda/image-lemgendizer-core.git"
    },
    "devDependencies": {
        "vite": "^5.4.21"
    },
    "peerDependencies": {
        "jszip": "^3.10.1"
    },
    "dependencies": {
        "jszip": "^3.10.1"
    }
}